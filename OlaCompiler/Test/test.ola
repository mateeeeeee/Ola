import std.assert;


int g = 9;
ref int ReturnRef()
{
	return g;
}
int ReturnValue()
{
	return g;
}

void TestRefReturnSimple()
{
	ref int gRef = ReturnRef();
	gRef++;
	Assert(g == 10);

	int gRefCopy = ReturnRef();
	gRefCopy++;
	Assert(g == 10);

	int gValue = ReturnValue();
	gValue++;
	Assert(g == 10);
}

public int main()
{
	TestRefReturnSimple();
	return 0;
}