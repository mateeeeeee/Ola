import std.io;


class Base 
{
	private int y = 10;
	
	public void PrintY()
	{
		PrintInt(y);
	}
};

class Derived extends Base
{
	public int x = 30;

	public void PrintX()
	{
		PrintY();
		PrintInt(x);
	}
};

void Test(Base b)
{
	b.PrintY();
}
void Test2(ref Base b)
{
	b.PrintY();
}

public int main()
{
	Derived d; 
	ref Derived refd = d;
	Test(refd);
	Test2(refd);
}