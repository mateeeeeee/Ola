import std.assert;

class Base
{
    int x = 10;
    int y = 20;
    public int GetX() virtual
    {
        return x;
    }
    public int GetY() virtual
    {
        return y;
    }
};

class Derived : Base
{
     int x = 100;
     public int GetX() virtual
     {
        return x;
     }
};

class ExtDerived : Derived
{
    int y = 200;
    public int GetY() virtual
    {
        return y; 
    }
    public int GetX(int) 
    {
        return GetY() + super.GetY();
    }
};

public int main() 
{
    ExtDerived d;
    ref Base b = d;
    return d.GetX(1);
}
