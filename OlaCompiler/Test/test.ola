import std.assert;

class Base
{
    int x = 10;
    int y = 20;
    public int GetX() virtual
    {
        return x;
    }
    public int GetY() virtual
    {
        return y;
    }
};

class Derived extends Base
{
     int x = 100;
     public int GetX() virtual
     {
        return x;
     }
};

class ExtDerived extends Derived
{
    int y = 200;
    public int GetY() virtual
    {
        return y; 
    }
    public int GetX(int) virtual
    {
        return 100000;
    }
};

public int main() 
{
    ExtDerived d;
    ref Base b = d;
    return b.GetX() + d.GetX(1);
}
