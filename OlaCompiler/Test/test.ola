import std.io;

class Base 
{
    int z = 10;
    int x = 123;

    public int GetX() virtual const {return x;}
    public int GetZ() virtual const {return z;}
};

class Derived extends Base
{
    int y = 1000;
    int x = 100;

    public int GetX() virtual const {return x;}
    public int GetY() virtual const {return y;}
};

public int main() 
{
    Derived d;
    ref Base b = d;
    PrintInt(d.GetX());
}
