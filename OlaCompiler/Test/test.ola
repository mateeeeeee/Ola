import std.assert;
import std.io;

class B
{
	B(int y)
	{
		this.y = y;
	}
	public int y = 0;
};

class D : B
{
	D(int x)
	{
		super(x * 2);
		this.x = x;
	}
	D(int x, int y)
	{	
		this(x);
		this.x *= y;
	}
	public int x = 0;
};

void Test(D d)
{
	PrintInt(d.x);
}

public int main() 
{
   D d1(5);
   Assert(d1.x == 5);
   Assert(d1.y == 10);

   D d2(2,3);
   Assert(d2.x == 6);
   Assert(d2.y == 4);

   return 0;
}
