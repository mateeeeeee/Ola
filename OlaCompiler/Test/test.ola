import std.assert;

class Base
{
    public int GetX() virtual pure;
};

class Derived : Base
{
     int y = 100;
     int x = 10;
     public int GetY() virtual
     {
        return y;
     }
     public int GetX() virtual
     {
        return x;
     }
};

public int main() 
{
    Derived d;
    return d.GetX();
}
