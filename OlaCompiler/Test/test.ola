import std.Assert;

class B
{
	B(int x)
	{
		this.x = x;
	}
	B(int x, int y)
	{
		this.x = x * y;
	}
	public int x = 0;
};

class D : B
{
	D(int x)
	{
		super(x);
		this.y = x;
	}
	D(int x, int y)
	{
		super(x,y);
		this.y = x * y;
	}
	public int y = 0;
};


public int main() 
{
   D d1(5);
   D d2(2,3);
   Assert(d1.y == 5);
   Assert(d2.y == 6);

   return 0;
}
