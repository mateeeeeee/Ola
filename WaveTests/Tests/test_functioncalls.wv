import std.assert;


private int square(int x)
{
	return x * x;
}

private int add(int a, int b)
{
	return a + b;
}

private bool isEven(int x)
{
	return x % 2 == 0;
}

private int fma(int a, int b, int c)
{
    return a * b + c;
}

private bool isInRange(int x, int lowerBound, int upperBound)
{
    return x >= lowerBound && x <= upperBound;
}

public int main()
{
	int result1 = square(5);
    Assert(result1 == 25);  

    bool result2 = isEven(8);
    Assert(result2 == true); 
    
    int result3 = square(-4);
    Assert(result3 == 16);  
    
    bool result4 = isEven(7);
    Assert(result4 == false);  
    
    int result5 = add(5,12);
    Assert(result5 == 17);  
    
    int fma1 = fma(2, 3, 4);
    Assert(fma1 == 10);  
    
    bool result6 = isInRange(5, 1, 10);
    Assert(result6 == true);  
    
    int fma2 = fma(6, 2, 8);
    Assert(fma2 == 20);  
    
    bool result7 = isInRange(5, 6, 10);
    Assert(result7 == false);  

    return 0;
}