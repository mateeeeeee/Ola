import std.assert;

// Test less than operators with integers and floats
void TestLessThanOperators()
{
    int intA = 5;
    int intB = 10;
    float floatC = 2.5;
    float floatD = 5.0;

    Assert(intA < intB);
    Assert(floatC < floatD);
}

// Test less than or equal operators with integers and floats
void TestLessThanOrEqualOperators()
{
    int intA = 5;
    int intB = 10;
    float floatC = 2.5;
    float floatD = 5.0;

    Assert(intA <= intB);
    Assert(floatC <= floatD);
}

// Test greater than operators with integers and floats
void TestGreaterThanOperators()
{
    int intA = 5;
    int intB = 10;
    float floatC = 2.5;
    float floatD = 5.0;

    Assert(intA > intB == false);
    Assert(floatC > floatD == false);
}

// Test greater than or equal operators with integers and floats
void TestGreaterThanOrEqualOperators()
{
    int intA = 5;
    int intB = 10;
    float floatC = 2.5;
    float floatD = 5.0;

    Assert(intA >= intB == false);
    Assert(floatC >= floatD == false);
}

// Test equal operators with integers and floats
void TestEqualOperators()
{
    int intA = 5;
    int intB = 10;
    float floatC = 2.5;
    float floatD = 5.0;

    Assert(intA == intB == false);
    Assert(floatC == floatD == false);
}

// Test not equal operators with integers and floats
void TestNotEqualOperators()
{
    int intA = 5;
    int intB = 10;
    float floatC = 2.5;
    float floatD = 5.0;

    Assert(intA != intB == true);
    Assert(floatC != floatD == true);
}

// Main testing function
public int main()
{
    // Test less than operators
    TestLessThanOperators();

    // Test less than or equal operators
    TestLessThanOrEqualOperators();

    // Test greater than operators
    TestGreaterThanOperators();

    // Test greater than or equal operators
    TestGreaterThanOrEqualOperators();

    // Test equal operators
    TestEqualOperators();

    // Test not equal operators
    TestNotEqualOperators();

    return 0;
}