import std.assert;

public int main()
{
	// Test relational operators with integers
    int a = 5;
    int b = 7;
    int c = 5;

    // Test less than (<)
    int less_than = a < b;
    Assert(less_than == 1);

    // Test less than or equal to (<=)
    int less_than_or_equal = a <= c;
    Assert(less_than_or_equal == 1);

    // Test equal to (==)
    int equal_to = a == c;
    Assert(equal_to == 1);

    // Test not equal to (!=)
    int not_equal_to = a != b;
    Assert(not_equal_to == 1);

    // Test relational operators with assignment
    int x = 10;
    int y = 15;

    // Test less than assignment (<)
    x = x < y;
    Assert(x == 1);

    int p = 8;
    int q = 5;

    // Test less than or equal to assignment (<=)
    p = p <= q;
    Assert(p == 0);

    int m = 5;
    int n = 5;

    // Test equal to assignment (==)
    m = m == n;
    Assert(m == 1);

    int g = 7;
    int h = 10;

    // Test not equal to assignment (!=)
    g = g != h;
    Assert(g == 1);

    return 0;
}