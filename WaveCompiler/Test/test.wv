import std.assert;
import std.io;

const float globalFloat = 10.0;

public int main()
{
	float localFloat = 3 * 3.3;
	Assert(globalFloat > localFloat);
	float diff = globalFloat - localFloat;
	float diffCubed = diff * diff * diff;
	int int1 = diffCubed * 999;
	Assert(int1 == 0);
	int int2 = diffCubed * 1000;
	Assert(int2 == 1);
	return 0;
}