import std.assert;

public class S
{
	public int x = 0;
	public int y = 0;

	public void SetX(int x) {this.x = x;}
	public void SetY(int y) {this.y = y;}

	public int GetX() const {return x;}
	public int GetY() const {return y;}
};

void ModifyS(S s)
{
	s.x *= 2;
	s.y *= 2;
}

public int main()
{
	S s; s.x = 10; s.y = 10;
	ModifyS(s);
	AssertMsg(s.x == 20, "1");
	AssertMsg(s.y == 20, "2");
	S s2 = s;
	AssertMsg(s2.GetX() == 20, "3");
	AssertMsg(s2.GetY() == 20, "4");
	s2.x += 10;
	AssertMsg(s.GetX() == 30, "5");
	AssertMsg(s2.GetX() == 30, "6");
	return 0;
}