import std.io;
import std.assert;


int g = 9;
ref int ReturnRef()
{
	return g;
}
int ReturnValue()
{
	return g;
}

void TestRefReturnSimple()
{
	ref int refG = ReturnRef();
	refG++;
	Assert(g == 10);

	ref int refGCopy = ReturnValue();
	refGCopy++;
	Assert(g == 10);

	int valueG = ReturnRef();
	valueG++;
	Assert(g == 10);

	int valueGCopy = ReturnValue();
	valueGCopy++;
	Assert(g == 10);
}

public int main()
{
	TestRefReturnSimple();
	return 0;
}