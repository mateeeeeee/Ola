import std.assert;

int TestIfElse()
{
	int a = 6;
	if (a > 5) return 10;
	else return 0;
}

int TestWhile()
{
	int a = 0;
	while (a < 10)
	{
		if (a == 5) return a * 2;
		++a;
	}
	return 0;
}

int TestMultipleReturns()
{
	int x = 5;
	if (x < 0) return -1;
	else if (x == 0) return 0;
	else if (x > 0) return 1;
	return -999;
}

int TestEarlyReturnInLoop()
{
	for (int i = 0; i < 10; i++)
	{
		if (i == 5) return i;
	}
	return -1;
}

int TestNestedReturn()
{
	for (int i = 0; i < 3; i++)
	{
		if (i == 1)
		{
			for (int j = 0; j < 2; j++)
			{
				if (j == 1) return i * j;
			}
		}
	}
	return -1;
}

void TestVoidWithEarlyReturn()
{
	int sum = 0;
	for (int i = 1; i <= 10; i++)
	{
		sum += i;
		if (sum > 30) return;
	}
	Assert(false);
}

public int main()
{
	Assert(TestIfElse() == 10);
	Assert(TestWhile() == 10);
	Assert(TestMultipleReturns() == 1);
	Assert(TestEarlyReturnInLoop() == 5);
	Assert(TestNestedReturn() == 1);
	TestVoidWithEarlyReturn();
	return 0;
}
