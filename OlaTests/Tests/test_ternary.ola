import std.assert;

// Test ternary operator with integers
void TestTernaryOperatorIntegers()
{
    int a = 5;
    int b = 10;

    int result = (a > b) ? a : b;
    Assert(result == 10);
}

// Test ternary operator with floats
void TestTernaryOperatorFloats()
{
    float c = 2.5;
    float d = 3.0;

    float result = (c > d) ? c : d;
    Assert(result == 3.0);
}

// Test ternary operator with bools
void TestTernaryOperatorBools()
{
    bool condition = true;
    int x = 5;
    int y = 10;

    int result = condition ? x : y;
    Assert(result == 5);
}

// Main testing function
public int main()
{
    // Test ternary operator with integers
    TestTernaryOperatorIntegers();

    // Test ternary operator with floats
    TestTernaryOperatorFloats();

    // Test ternary operator with bools
    TestTernaryOperatorBools();

    return 0;
}