int compute(int a, int b)
 {
     int x = a * b + 10;  
     if (a > 0)
     {
         int y = a * b + 10;  
         return y + x;        
     }
     else
     {
         // Another basic block
         int z = a * b + 10;  
         return z - x;        
     }
 }

 public int main()
 {
     int result1 = compute(5, 3);   
     int result2 = compute(-1, 4);  
     return result1 + result2;      
 }