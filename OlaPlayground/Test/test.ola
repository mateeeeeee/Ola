import std.assert;

public int main()
{
	int[5] arr = {1, 2, 3, 4, 5};

	int sum = 0;
	for (int i = 0; i < 5; i++)
	{
		sum += arr[i];
	}
	Assert(sum == 15);

	// Modify in loop
	for (int i = 0; i < 5; i++)
	{
		arr[i] *= 2;
	}
	Assert(arr[0] == 2);
	Assert(arr[1] == 4);
	Assert(arr[2] == 6);
	Assert(arr[3] == 8);
	Assert(arr[4] == 10);

	// Find max
	int max = arr[0];
	for (int i = 1; i < 5; i++)
	{
		if (arr[i] > max)
		{
			max = arr[i];
		}
	}
	return max;
}