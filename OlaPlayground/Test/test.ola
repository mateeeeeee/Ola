import std.assert;

void TestNestedDoWhileLoops()
{
    int outerCounter = 0;
    int innerCounter = 0;

    do
    {
        do
        {
            innerCounter++;
        } while (innerCounter < 2);

        outerCounter++;
    } while (outerCounter < 3);

    Assert(outerCounter == 3);
    Assert(innerCounter == 4);
}


public int main()
{
    TestNestedDoWhileLoops();
    return 0;
}