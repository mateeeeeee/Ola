import std.assert;

int f() {return 3;}

public int main()
{
	int a = 1 + 2; // Compute sum
    int b = f();
    int diff = a - b;  
    if (diff == 0)
    {
        int diff2 = a - b; // Redundant computation of a - b
        Assert(diff2 == 0); // Use diff2
    }
    else
    {
        Assert(false);
    }
    return 0;
}