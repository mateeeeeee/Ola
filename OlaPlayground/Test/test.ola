import std.assert;

public int main()
{
    char c = 'b';
    int result = 0;
    
    switch (c)
    {
        case 'a':
            result = 1;
            break;
        case 'b':
            result = 2;
            break;
        case 'c':
            result = 3;
            break;
        default:
            result = 0;
    }
    
    Assert(result == 2);
    return 0;
}