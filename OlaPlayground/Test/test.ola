import std.assert;

int TestGotoErrorHandling()
{
    int result = 0;
    int errorCode = 1;
    if (errorCode != 0)
    {
        goto HandleError;
    }

    result = 42;
    return result;

HandleError:
    result = -1;
    return result;
}


public int main()
{
	int errorHandlingResult = TestGotoErrorHandling();
    Assert(errorHandlingResult == -1);
    return 0;
}